<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/divider2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider" />

    <TableRow
        android:id="@+id/colRecaudo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="clickRecaudoMostrar"
        android:paddingStart="20dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp">

            <TextView
                android:id="@+id/nameClient"
                android:layout_width="173dp"
                android:layout_height="0dp"
                android:layout_marginStart="4dp"
                android:text="TextView"
                android:textColor="@color/azulgris치ceo"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteY="0dp" />

            <TextView
                android:id="@+id/method_payment"
                android:layout_width="195dp"
                android:layout_height="0dp"
                android:layout_marginTop="20dp"
                android:text="TextView"
                android:textColor="@color/azulgris치ceo"
                app:layout_constraintEnd_toStartOf="@+id/collect_date"
                app:layout_constraintHorizontal_bias="0.076"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/collect_date"
                android:layout_width="134dp"
                android:layout_height="0dp"
                android:layout_marginEnd="16dp"
                android:text="TextView"
                android:textColor="@color/azulgris치ceo"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />

            <TextView
                android:id="@+id/collect_status"
                android:layout_width="139dp"
                android:layout_height="0dp"
                android:text="TextView"
                android:textColor="@color/azulgris치ceo"
                app:layout_constraintBottom_toTopOf="@+id/collect_date"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.23"
                app:layout_constraintStart_toEndOf="@+id/nameClient" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </TableRow>

</androidx.constraintlayout.widget.ConstraintLayout>