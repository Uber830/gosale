<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sena.gosale">
    //permisos
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key"/>
        <activity
            android:name=".ClientesLocalizacionActivity"
            android:exported="true" />
        <activity
            android:name=".ClientesInformacionDescripcionActivity"
            android:exported="true" />
        <activity
            android:name=".ClientesInformacionActivity"
            android:exported="true" />
        <activity
            android:name=".ClientesActivity"
            android:exported="true" />
        <activity
            android:name=".productsAnatomyActivity"
            android:exported="true" />
        <activity
            android:name=".ProductsLineActivity"
            android:exported="true" />
        <activity
            android:name=".productsActivity"
            android:exported="true" />
        <activity
            android:name=".loguinActivity"
            android:exported="true"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>